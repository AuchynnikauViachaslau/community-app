{
  "name": "community-app-server",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "webpack --config webpack.config.js --env=dev",
    "prestart": "npm run build && node prestart.js",
    "start": "node ./build/community-app-server.app.bundle.js",
    "preprod": "npm test",
    "prod": "webpack --config webpack.config.js --env=prod",
    "test": "jest --config jest.config.json --forceExit",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.8.6",
    "@types/mongoose": "^5.0.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.13",
    "@types/socket.io": "^1.4.33",
    "@types/supertest": "^2.0.4",
    "@types/verror": "^1.10.3",
    "awesome-typescript-loader": "^4.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "jest": "^22.4.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^3.9.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "inversify": "^4.11.1",
    "inversify-express-utils": "^5.2.1",
    "inversify-inject-decorators": "^3.1.0",
    "inversify-logger-middleware": "^3.1.0",
    "mongoose": "^5.0.10",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.1.0",
    "verror": "^1.10.0",
    "winston": "^3.0.0-rc6"
  }
}
